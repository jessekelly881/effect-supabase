{
    "$schema": "https://biomejs.dev/schemas/1.7.3/schema.json",
    "linter": {
        "enabled": true,
        "rules": {
            "complexity": {
                "noStaticOnlyClass": "off"
            },
            "recommended": true,
            "suspicious": {
                "noExplicitAny": "off"
            }
        }
    },
    "organizeImports": {
        "enabled": true
    }
}
