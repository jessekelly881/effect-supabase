{
  "$schema": "https://raw.githubusercontent.com/SchemaStore/schemastore/master/src/schemas/json/package.json",
  "name": "effect-supabase",
  "version": "0.0.13",
  "description": "An Effect wrapper for Supabase",
  "private": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "homepage": "https://github.com/jessekelly881/effect-supabase",
  "tags": [
    "typescript",
    "effect",
    "effect-ts",
    "supabase",
    "supabase-js",
    "supabase-db"
  ],
  "keywords": [
    "typescript",
    "effect",
    "effect-ts",
    "supabase",
    "supabase-js",
    "supabase-db"
  ],
  "scripts": {
    "build": "tsup src/**/*.ts --format cjs,esm --dts",
    "lint": "eslint 'src/*.{ts,tsx}'",
    "dev": "vitest --bail 1 --reporter=dot",
    "test": "vitest run --bail 1 --reporter=basic",
    "ci": "pnpm run lint && pnpm run test && pnpm run build",
    "publish": "changeset publish",
    "docs": "docgen",
    "gen": "plop",
    "dtslint": "dtslint dtslint",
    "circular": "madge --extensions ts --circular --no-color --no-spinner src",
    "check": "tsc -b tsconfig.json"
  },
  "author": "Jesse Kelly",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@effect/docgen": "^0.4.3",
    "@effect/dtslint": "^0.1.0",
    "@effect/platform": "^0.52.1",
    "@effect/schema": "^0.66.13",
    "@effect/vitest": "^0.4.0",
    "@fast-check/vitest": "^0.1.0",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "bun": "^1.1.4",
    "effect": "^3.0.5",
    "eslint": "^9.0.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "fast-check": "^3.17.1",
    "globals": "^15.0.0",
    "husky": "^9.0.7",
    "jsdom": "^24.0.0",
    "madge": "^7.0.0",
    "msw": "^2.3.0",
    "plop": "^4.0.0",
    "tsup": "^8.0.1",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.7.0",
    "vite": "^5.2.9",
    "vitest": "^1.5.0"
  },
  "dependencies": {
    "@supabase/postgrest-js": "^1.15.2",
    "@supabase/supabase-js": "^2.42.5"
  },
  "peerDependencies": {
    "@effect/platform": "^0.50.2",
    "@effect/schema": "^0.66.5",
    "effect": "^3.0.3"
  }
}