{
  "$schema": "https://raw.githubusercontent.com/SchemaStore/schemastore/master/src/schemas/json/package.json",
  "name": "effect-supabase",
  "version": "0.0.7",
  "description": "An Effect wrapper for Supabase",
  "private": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "homepage": "https://github.com/jessekelly881/effect-supabase",
  "tags": [
    "typescript",
    "effect",
    "effect-ts",
    "supabase",
    "supabase-js",
    "supabase-db"
  ],
  "keywords": [
    "typescript",
    "effect",
    "effect-ts",
    "supabase",
    "supabase-js",
    "supabase-db"

  ],
  "scripts": {
    "build": "tsup src/**/*.ts --format cjs,esm --dts",
    "lint": "eslint 'src/*.{ts,tsx}'",
    "dev": "vitest --bail 1 --reporter=dot",
    "test": "vitest run --bail 1 --reporter=basic",
    "ci": "pnpm run lint && pnpm run test && pnpm run build",
    "publish": "changeset publish",
    "docs": "docgen",
    "gen": "plop",
    "dtslint": "dtslint dtslint",
    "circular": "madge --extensions ts --circular --no-color --no-spinner src",
    "check": "tsc -b tsconfig.json"
  },
  "author": "Jesse Kelly",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@effect/docgen": "^0.4.0",
    "@effect/vitest": "^0.2.3",
    "@fast-check/vitest": "^0.1.0",
    "@types/node": "^20.11.28",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "bun": "^1.0.32",
    "eslint": "^8.51.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "fast-check": "^3.14.0",
    "husky": "^9.0.7",
    "jsdom": "^24.0.0",
    "madge": "^6.1.0",
    "plop": "^4.0.0",
    "tsup": "^8.0.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vitest": "^1.4.0"
  },
  "dependencies": {
    "@effect/dtslint": "^0.1.0",
    "@effect/platform": "^0.48.22",
    "@effect/schema": "^0.64.16",
    "@supabase/postgrest-js": "^1.9.2",
    "@supabase/supabase-js": "^2.41.1",
    "effect": "^2.4.15"
  }
}
